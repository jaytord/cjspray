package cfm.utils.youtube.events {
	/**
	 * A collection of event constants related to the YouTube API.
	 * 
	 * <p>Note that this class does not inherit from <code>Event</code> since
	 * the event object is dispatched solely from the loaded player.</p>
	 */
	public class PlayerEvent {
		static public const READY:String = "onReady";
		static public const ERROR:String = "onError";
		static public const STATE_CHANGE:String = "onStateChange";
		static public const PLAYBACK_QUALITY_CHANGE:String = "onPlaybackQualityChange";
	}
}